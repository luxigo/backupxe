prompt 0
noescape 1
default menu.c32
menu master passwd pxemaster
timeout 100
ontimeout menu.c32 pxeboot.cfg/boot

menu title eMoviX

#default MoviX
#prompt 1
#timeout 70
#display movix.msg
#kbdmap bootkey.map
#F1 mxhelp.txt
#F2 trblst.txt
#F3 mphelp.txt
#F4 credits.txt
#F5 trblst2.txt
#F6 expert1.txt
#F7 expert2.txt

label MoviX
  menu label MoviX
  kernel boot/emovix/vmlinuz
  append LABEL=MoviX video=vesa splash=silent vga=0x314 initrd=boot/emovix/initrd.gz load_ramdisk=1 prompt_ramdisk=0 ramdisk_size=14342 rw root=/dev/ram0
label TV
  menu label TV
  kernel boot/emovix/vmlinuz
  append LABEL=TV video=vesa:off splash=silent initrd=boot/emovix/initrd.gz load_ramdisk=1 prompt_ramdisk=0 ramdisk_size=14342 rw root=/dev/ram0
