prompt 0
noescape 1
default menu.c32
timeout 100
ontimeout menu.c32 pxelinux.cfg/deploy

menu title unattended - DOS bootdisks

label b44
    menu label b44
    kernel memdisk
    append initrd=boot/unattended/images/b44.imz

